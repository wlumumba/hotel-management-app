<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2d3349;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.singleReservationRoomController">
   <children>
      <TabPane prefHeight="500.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="Reservation">
               <content>
                  <AnchorPane fx:id="singleReservationRoom" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="table_hotelR2" layoutY="-3.0" prefHeight="313.0" prefWidth="634.0" style="-fx-background-color: #a7b4c6;">
                           <columns>
                              <TableColumn fx:id="col_roomidR" prefWidth="167.0" text="Room #" />
                              <TableColumn fx:id="col_type1" minWidth="0.0" prefWidth="0.0" />
                              <TableColumn fx:id="col_bedtypeR" minWidth="0.0" prefWidth="175.0" text="Room Type" />
                              <TableColumn fx:id="col_roompriceR" prefWidth="127.0" text="Price" />
                           </columns>
                        </TableView>
                        <TextField fx:id="roomR" layoutX="115.0" layoutY="311.0" prefHeight="27.0" prefWidth="194.0" style="-fx-background-color: transparent; -fx-border-color: #2f60d0; -fx-border-width: 0px 0px 2px 0px; -fx-text-fill: white;">
                           <font>
                              <Font name="Lucida Sans Unicode" size="14.0" />
                           </font></TextField>
                        <Label layoutX="14.0" layoutY="314.0" prefHeight="27.0" prefWidth="137.0" text="Enter Room #:" textFill="#2f60d0">
                           <font>
                              <Font name="Lucida Sans Unicode" size="16.0" />
                           </font></Label>
                        <Label layoutX="328.0" layoutY="314.0" prefHeight="27.0" prefWidth="98.0" text="Enter Email:" textFill="#2f60d0">
                           <font>
                              <Font name="Lucida Sans Unicode" size="16.0" />
                           </font></Label>
                        <TextField fx:id="emailR" layoutX="418.0" layoutY="309.0" prefHeight="31.0" prefWidth="172.0" style="-fx-background-color: transparent; -fx-border-color: #2f60d0; -fx-border-width: 0px 0px 2px 0px; -fx-text-fill: white;">
                           <font>
                              <Font name="Lucida Sans Unicode" size="14.0" />
                           </font></TextField>
                        <Button fx:id="reserveButton" layoutX="270.0" layoutY="369.0" mnemonicParsing="false" onAction="#reserveButton" style="-fx-background-color: #2f60d0;" text="Reserve" textFill="WHITE">
                           <font>
                              <Font name="Lucida Sans Unicode" size="16.0" />
                           </font></Button>
                        <Button fx:id="returnButton" layoutX="711.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnButton" prefHeight="31.0" prefWidth="75.0" style="-fx-background-color: #2f60d0;" text="Return" textFill="WHITE">
                           <font>
                              <Font name="Lucida Sans Unicode" size="16.0" />
                           </font></Button>
                     </children></AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
